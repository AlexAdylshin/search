{
  "name": "burzum/cakephp-search-plugin",
  "description": "CakePHP 3.0 easy PRG search framework",
  "type": "cakephp-plugin",
  "keywords": [
    "cakephp",
    "cakephp3",
    "cake3",
    "search",
    "post-redirect-get",
    "prg"
  ],
  "homepage": "https://github.com/burzum/cakephp-search-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "Christian Winther",
      "role": "Author",
      "homepage": "http://cakephp.nu/"
    },
    {
      "name": "Florian Kr√§mer",
      "role": "Author",
      "homepage": "http://florian-kraemer.net/"
    }
  ],
  "require-dev": {
    "cakephp/cakephp": "~3.0",
    "friendsofcake/cakephp-test-utilities": "dev-master",
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "Burzum\\Search\\": "src",
      "Burzum\\Search\\Test\\Fixture\\": "tests\\Fixture"
