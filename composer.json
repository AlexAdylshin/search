    }
  },
  "support": {
    "source": "https://github.com/FriendsOfCake/search",
    "issues": "https://github.com/FriendsOfCake/search/issues",
    "wiki": "http://cakephp.nu/cakephp-search/",
    "irc": "irc://irc.freenode.org/friendsofcake"
  }
